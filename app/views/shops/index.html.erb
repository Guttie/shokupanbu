<main>
  <h2 class="title">SHOPS</h2>
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <% @shops.shop_images.first(1).each do |image| %>
          <div id="slider">
            <%= attachment_image_tag image, :image, :fill, 350, 350, size: "350x350" %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</main>